    }    
}, 100);
  
  
function resize_chat()
{
 var gorgiasChatInterval = window.setInterval(function () {

     try { 
     if (Smooch) {
       setTimeout(function() { 
          window.clearInterval(gorgiasChatInterval); // this line breaks out of the loop - make sure it's not deleted.        
          // you can also use widget:open here's the complete list of events: https://github.com/gorgias/smooch-js#events
          Smooch.on('widget:opened', function() {
              // your code should go here
            document.getElementById('gorgias-web-messenger-container').style.bottom = '0';

          });
          Smooch.on('widget:closed', function() {
              // your code should go here
            document.getElementById('gorgias-web-messenger-container').style.bottom = '70px';

          });
         }, 300);
      }
    } catch(err) {}  
  }, 200); 
} 
</script>
  
