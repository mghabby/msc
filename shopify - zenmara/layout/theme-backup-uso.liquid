<script type="text/javascript">
	{% if template contains 'product' %}
		gtag('event', 'page_view', {
		ecomm_prodid: 'shopify_FR_{{ product.id }}_{{ product.variants.first.id }}',
		ecomm_pagetype: 'product',
		ecomm_totalvalue: {{ product.price | money_without_currency | replace: ',', '.' }}
		});
	{% else if template contains 'cart' %}
		gtag('event', 'page_view', {
		ecomm_prodid: [{% for item in cart.items %}'shopify_FR_{{ item.product.id }}_{{ item.product.variants.first.id }}',{% endfor %}],
		ecomm_pagetype:  'cart',
		ecomm_totalvalue: {{cart.total_price | money_without_currency | replace: ',', '.' }}
		});
	{% else if template contains 'search' %}
		gtag('event', 'page_view', {
		ecomm_prodid: '',
		ecomm_pagetype: 'searchresults',
		ecomm_totalvalue: ''
		});
	{% else if template contains 'index' %}
		gtag('event', 'page_view', {
		ecomm_prodid: '',
		ecomm_pagetype: 'home',
		ecomm_totalvalue: ''
		});
	{% else %}
		gtag('event', 'page_view', {
		ecomm_prodid: '',
		ecomm_pagetype: 'other',
		ecomm_totalvalue: ''
		});
	{% endif %}
</script>




